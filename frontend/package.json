{
  "name": "finehero-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "security-audit": "npm audit --audit-level=moderate",
    "security-audit-json": "npm audit --audit-level=moderate --json > security-audit-report.json",
    "security-fix": "npm audit fix",
    "dependency-check": "npm outdated || echo 'All dependencies are up to date'",
    "license-check": "npx license-checker --json > license-report.json",
    "sbom": "npx @cyclonedx/bom --output-format json --output-file sbom.json"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "axios": "^1.6.2",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.0.18",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "react-hot-toast": "^2.4.1",
    "lucide-react": "^0.302.0",
    "recharts": "^2.10.3",
    "react-query": "^3.39.3",
    "@tanstack/react-query": "^5.17.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "eslint-plugin-security": "^2.1.0",
    "license-checker": "^25.0.1",
    "@cyclonedx/bom": "^4.0.0",
    "audit-ci": "^6.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "audit": {
    "auditLevel": "moderate",
    "advisory": "https://www.npmjs.com/advisories"
  }
}